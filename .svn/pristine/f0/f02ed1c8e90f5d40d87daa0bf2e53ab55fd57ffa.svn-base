#include "object_construction.hpp"

obj::obj() {
    std::cout << "Called Defualt Ctor" << std::endl;
}

obj::~obj() {
    std::cout << "Called Destructor" << std::endl;
}

obj::obj(const obj& copy) {
    std::cout << "Called Copy Ctor" << std::endl;
}

obj& obj::operator=(const obj& rhs) {
    obj Object;
    std::cout << "Called assigment" << std::endl;
    return Object;
}